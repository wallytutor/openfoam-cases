FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
        solver          GAMG;
        smoother        GaussSeidel;
        tolerance       1.0e-07;
        relTol          0.1;
    }

    "(U|e|h|k|omega|epsilon)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1.0e-07;
        relTol          0.1;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    pMinFactor      0.1;
    pMaxFactor      1.5;

    transonic       no;
    consistent      no;

    residualControl
    {
        p                   1.0e-04;
        U                   1.0e-04;
        "(e|h)"             1.0e-04;
        "(k|omega|epsilon)" 1.0e-03;
    }
}

relaxationFactors
{
    fields
    {
        p               0.3;
    }
    equations
    {
        U               0.7;
        e               0.7;
        h               0.7;
        k               0.7;
        omega           0.7;
        epsilon         0.7;
    }
}

